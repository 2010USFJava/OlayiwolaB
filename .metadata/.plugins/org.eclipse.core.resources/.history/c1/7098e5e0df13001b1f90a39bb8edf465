package com.revature.driver;

import com.revature.beans.*;

public class Driver {

	public static void main(String[] args) {
		Purse p = new Purse("Gucchi", "Medium");
		Bag b = new Bag(400, "red", p);
		Purse p2 = new Purse();
		Bag b2 = new Bag();
		
		System.out.println(p + "\n" + b + "\n");
		System.out.println(p2 + "\n" + b2);
	}

}
