package com.revature.tryclasswork;


public class MethodOperationOrder {
	
	static int myInt =3;
	static int myInt2 =3;
	static Integer myInteger =5;
	static Integer myInteger2 =5;
	static Double myDouble = 40.93;
	
	public static int addUp(int a, int b, int c, int d) {//
		return a+b+c+d;
	}
	public static int addUp(int a, int b, int c) {//
		return a+b+c;
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
	//int result =addEmup(myInt,myInt2, myInteger, myInteger2);//wrapper form W to P
		int result =addUp(myInt, myInteger2, myInt2, myDouble.intValue());//by adding utility int method "conversion"
		int result1 =addUp(myInt, myInteger2, myInt2);
		System.out.println(result1);
		System.out.println(result);

		
		vaTest(10,6,7);
	
		
	}
	
	static void  vaTest(int ... v ) {//varargs must be last args, and args can be of anytype
		System.out.println("Number of args is : " +  v.length  + " contents:");
		for(int x: v) {
			System.out.println(x);
		}
	}
	}


