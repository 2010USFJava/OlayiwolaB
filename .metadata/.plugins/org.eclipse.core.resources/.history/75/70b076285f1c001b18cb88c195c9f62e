package com.revature.string;

public class Stringsnipt {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s1 ="Hello Everyone";
		System.out.println(s1.substring(0,6) + "World!");
		
		String d ="What is for dinner?";
		System.out.println(d.substring(5));
		
		String s="What Time is It?";
		System.out.println("s");
	}

}
