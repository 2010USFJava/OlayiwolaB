package com.revature.generics;
/*
 * Generics
 * 	- using a type**/
public class Gen1 <T>{
	
	//DECLARE OBJECT OF TYPE T
	T object;
	
	//pass a constructor referencing to object of type T
	Gen1(T objectParameter){
		object = objectParameter;
		
	}
	//return object
	T getObject(){
		return object;
	}
	//SHOW THE TYPE OF t
	void showType() {
		System.out.println("Type of T is : "+object.getClass().getName());
	}
}
