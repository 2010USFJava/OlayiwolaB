import java.util.ArrayList;
import java.util.Arrays;
import java.util.Collections;
import java.util.List;

import com.revature.compare.Student;

public class CompareMain {

	public static void main(String[] args) {
		// TStudent<implement>o-generated method stub
		List<Student>studentList = new ArrayList<Student>();
		studentList.addAll(Arrays.asList(
				new Student[] {
						new Student(9,"matt",7.22),
						new Student(9,"ola",5.22),
						new Student(9,"yomi",4.22),
						new Student(9,"mic",6.22),
						new Student(9,"nikky",8.22),
						new Student(9,"solo",1.22),
						new Student(9,"deola",3.22),
						new Student(9,"ade",0.22)
		}));
		System.out.println("Original list");
		for(student s: studentList) {
			System.out.println(s);
		}
		System.out.println("==============");
		Collections.sort(studentList);	
		for(student s: studentList) {
			System.out.println(s);
	}

}
